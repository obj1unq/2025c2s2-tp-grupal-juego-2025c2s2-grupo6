import wollok.game.*
import personaje.*
import obstaculos.*


program fallToPieces{
    game.width(7)
    game.height(10)
    game.cellSize(50)
    game.title("fall to pieces")
    // game.boardGround(image)
    game.addVisual(personaje)
    game.addVisual(obstaculo)
    
    game.onTick(1000, "caer", {obstaculo.caer()})
    
    game.start()
    

}
